<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.pemudagabut.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>
    <permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/NoActionBar">
        <activity android:name=".activity.UpdateProfileScreenActivity"></activity>
        <activity android:name=".activity.TentangScreenActivity" />
        <activity android:name=".activity.ProfileScreenActivity" />
        <activity android:name=".activity.DaftarScreenActivity" />
        <activity android:name=".activity.MasukScreenActivity" />
        <activity android:name=".activity.StartScreenActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>